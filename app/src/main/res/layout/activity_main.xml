<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="#F8F8FF"
    tools:context=".MainActivity">


    <ImageButton
        android:layout_width="100dp"
        android:layout_height="120dp"
        android:id="@+id/exercises_button"
        android:background="@drawable/new_exercises"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="30dp"
        android:layout_marginStart="25dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="120dp"
        android:id="@+id/schedule_button"
        android:background="@drawable/new_schedule"
        android:layout_alignTop="@+id/exercises_button"
        android:layout_toRightOf="@+id/exercises_button"
        android:layout_toEndOf="@+id/exercises_button"
        android:layout_marginLeft="80dp"
        android:layout_marginStart="39dp" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="120dp"
        android:id="@+id/diet_button"
        android:background="@drawable/new_diet"
        android:layout_marginTop="180dp"
        android:layout_toLeftOf="@+id/schedule_button"
        android:layout_toStartOf="@+id/schedule_button" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="120dp"
        android:id="@+id/settings_button"
        android:background="@drawable/new_settings"
        android:layout_alignTop="@+id/diet_button"
        android:layout_alignLeft="@+id/schedule_button"
        android:layout_alignStart="@+id/schedule_button" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="120dp"
        android:id="@+id/instructor_button"
        android:background="@drawable/new_instructor"
        android:layout_below="@+id/diet_button"
        android:layout_alignLeft="@+id/diet_button"
        android:layout_alignStart="@+id/diet_button"
        android:layout_marginTop="30dp" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="120dp"
        android:id="@+id/supplement_button"
        android:background="@drawable/new_supplements"
        android:layout_alignTop="@+id/instructor_button"
        android:layout_alignRight="@+id/settings_button"
        android:layout_alignEnd="@+id/settings_button" />
</RelativeLayout>
